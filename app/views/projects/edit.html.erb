<% provide(:title, 'Edit Project - Qtrack') %>

<!-- Main content -->
<div class="container" role="main">

  <!-- Breadcrumbs -->
  <ul class="breadcrumb">
    <li>
      <%= link_to root_path do %>
          <span class="awe-home"></span> Home
      <% end %>
    </li>
    <li>
      <%= link_to projects_path do %>
          <span class="awe-tags"></span> Projects
      <% end %>
    </li>
    <li class="active">Edit project</li>
  </ul>
  <!-- /Breadcrumbs -->

  <!-- Display flashes here -->
  <%= render_flashes %>

  <div class="content">
    <div class="page-header">
      <h2>Edit project <small> Use this form to edit an existing project.</small></h2>
    </div>
    <!-- Tab content -->
    <div class="page-container tab-content">
      <!-- Tab #ui -->
      <div class="tab-pane active">
        <%= render partial: 'form', locals: {is_edit: true} %>
      </div>
    </div>
  </div>
</div>
<!-- /Main content -->